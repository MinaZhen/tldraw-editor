datasource db {
  provider = "sqlite"
  url      = "file:../dev.db"
}

generator client {
  provider = "prisma-client-js"
  output   = "../../../../node_modules/.prisma/client"
}

model TldrawData {
  id          String   @id @default(uuid())
  name        String
  storeData   Json?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}